{
    "name": "programster/pgsql-lib",
    "type": "library",
    "description": "A library for interfacing with a PostgreSQL database in PHP.",
    "keywords": ["pgsql", "postgresql"],
    "homepage": "https://github.com/programster/package-pgsql-libs",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://blog.programster.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-pgsql": "*"
    },
    "require-dev" : {
        "irap/autoloader" : "1.0.*",
        "programster/core-libs": ">=2.1 <3.0",
        "programster/pgsql-migrations": ">=0.3 <2.0"
    },
    "autoload": {
        "psr-4": {
            "Programster\\PgsqlLib\\": "src/",
            "Programster\\PgsqlLib\\Exceptions\\": "src/exceptions"
        }
    }
}
